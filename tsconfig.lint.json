{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "bundler",
      "jsx": "react-jsx",
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"]
      },
      "types": ["vite/client"]
    },
    "include": [
      "src",
      "vite.config.ts",
      "tailwind.config.ts"
    ]
    
  }
  